<Window 
	x:Class="VidCoder.View.UpdateSuccess"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:prop="clr-namespace:VidCoder.Properties"
	xmlns:res="clr-namespace:VidCoder.Resources"
	Title="{x:Static res:MiscRes.UpdateSuccessDialogTitle}" Height="119" Width="290" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
	TextOptions.TextFormattingMode="Display">
	<Grid>
		<TextBlock Text="{x:Static res:MiscRes.UpdateSuccessDialogMessage}" VerticalAlignment="Top" Margin="19,12,12,0" TextWrapping="Wrap" />
		<Button
			Content="{x:Static res:CommonRes.Ok}" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" VerticalAlignment="Bottom" Width="75" IsDefault="True" Click="button1_Click" />
	</Grid>
</Window>
