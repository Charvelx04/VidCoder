<Window x:Class="VidCoder.View.ScanMultipleDialog"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Scanning files..." Height="90" Width="426" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow" ShowInTaskbar="False" Closing="Window_Closing">
	<Grid>
		<ProgressBar
			Height="23" Margin="12,12,12,0" Name="progressBar1" VerticalAlignment="Top"
			Value="{Binding Progress, Mode=OneWay}"/>
	</Grid>
</Window>
