<Window x:Class="VidCoder.View.ChoosePresetName"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Style="{StaticResource NormalWindow}"
	Title="Name Preset" Height="152" Width="250" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" ShowInTaskbar="False" Loaded="Window_Loaded">
	<Grid>
		<TextBlock Height="21" Margin="12,12,0,0" Name="textBlock1" VerticalAlignment="Top" Text="Choose a name for the preset:" HorizontalAlignment="Left" Width="193" />
		<TextBox Margin="12,39,12,0" Name="presetNameText" Text="{Binding DisplayName, UpdateSourceTrigger=PropertyChanged}" Height="23" VerticalAlignment="Top" />
		<Button Height="23" Margin="0,0,93,12" Name="button1" VerticalAlignment="Bottom" IsDefault="True"
			 Command="{Binding AcceptCommand}" HorizontalAlignment="Right" Width="78">OK</Button>
		<Button Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="button2" VerticalAlignment="Bottom" Width="75" IsCancel="True"
			 Command="{Binding CancelCommand}">Cancel</Button>
	</Grid>
</Window>
